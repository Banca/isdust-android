<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <com.formal.sdusthelper.SlideMenu
        android:id="@+id/slide_menu"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

        <include layout="@layout/layout_menu" />

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@color/color_page_white"
            android:orientation="vertical" >

            <include layout="@layout/layout_title_bar"
                android:id="@+id/include2" />

            <Button
                android:id="@+id/FormJiaowu_button_emptyroom"
                android:onClick="onFormJiaowuClick"
                android:background="@drawable/emptyroom"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_below="@+id/FormJiaowu_button_schedule"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

           <Button
                android:id="@+id/FormJiaowu_button_schedule"
                android:onClick="onFormJiaowuClick"
                android:background="@drawable/courelist"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_below="@+id/include2"
                android:layout_centerInParent="true"/>

        </RelativeLayout>
    </com.formal.sdusthelper.SlideMenu>

</RelativeLayout>