<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:background="#f5f6f7"
                android:id="@+id/relativelayout_schedule_add_zhoushu"
                android:gravity="center"
    >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="周数选择"
        android:id="@+id/textView33" android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true" android:layout_alignParentStart="true"
        android:background="#f6f6f7" android:textColor="#5b5b5b"/>
    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_below="@+id/textView33" android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" android:id="@+id/tableLayout"
        android:background="#f5f6f7">
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </TableRow>
        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="fill_parent">
            <TextView
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="1"
                android:id="@+id/textView_zhoushu_1" android:layout_column="5"
                android:gravity="center" android:layout_margin="1dp" android:background="#ffffff"/>
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="2"
                      android:id="@+id/textView_zhoushu_2" android:layout_column="5"
                      android:gravity="center" android:layout_below="@+id/textView33"
                      android:layout_toRightOf="@+id/tableLayout"
                      android:layout_toEndOf="@+id/tableLayout" android:layout_margin="1dp"
                      android:background="#ffffff"/>
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="3"
                      android:id="@+id/textView_zhoushu_3" android:layout_column="5"
                      android:gravity="center" android:layout_below="@+id/textView33"
                      android:layout_toRightOf="@+id/tableLayout"
                      android:layout_toEndOf="@+id/tableLayout" android:layout_margin="1dp"
                      android:background="#ffffff"/>
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="4"
                      android:id="@+id/textView_zhoushu_4" android:layout_column="5"
                      android:gravity="center" android:layout_below="@+id/textView33"
                      android:layout_toRightOf="@+id/tableLayout"
                      android:layout_toEndOf="@+id/tableLayout" android:layout_margin="1dp"
                      android:background="#ffffff"/>
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="5"
                      android:id="@+id/textView_zhoushu_5" android:layout_column="5"
                      android:gravity="center" android:layout_below="@+id/textView33"
                      android:layout_toRightOf="@+id/tableLayout"
                      android:layout_toEndOf="@+id/tableLayout" android:layout_margin="1dp"
                      android:background="#ffffff"/>
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="6"
                      android:id="@+id/textView_zhoushu_6" android:layout_column="5"
                      android:gravity="center" android:layout_margin="1dp"
                      android:background="#ffffff"/>
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="7"
                      android:id="@+id/textView_zhoushu_7" android:layout_column="6"
                      android:gravity="center" android:layout_margin="1dp"
                      android:background="#ffffff"/>
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="8"
                      android:id="@+id/textView_zhoushu_8" android:layout_column="7"
                      android:gravity="center" android:layout_margin="1dp"
                      android:background="#ffffff"/>
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="9"
                      android:id="@+id/textView_zhoushu_9" android:layout_column="8"
                      android:gravity="center" android:layout_margin="1dp"
                      android:background="#ffffff"/>
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="10"
                      android:id="@+id/textView_zhoushu_10" android:layout_column="9"
                      android:gravity="center" android:layout_margin="1dp"
                      android:background="#ffffff"/>
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="11"
                      android:id="@+id/textView_zhoushu_11" android:layout_column="5"
                      android:gravity="center" android:layout_margin="1dp"
                      android:background="#ffffff"/>
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="12"
                      android:id="@+id/textView_zhoushu_12" android:layout_column="6"
                      android:gravity="center" android:layout_margin="1dp"
                      android:background="#ffffff"/>
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="13"
                      android:id="@+id/textView_zhoushu_13" android:layout_column="7"
                      android:gravity="center" android:layout_margin="1dp"
                      android:background="#ffffff"/>
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="14"
                      android:id="@+id/textView_zhoushu_14" android:layout_column="8"
                      android:gravity="center" android:layout_margin="1dp"
                      android:background="#ffffff"/>
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="15"
                      android:id="@+id/textView_zhoushu_15" android:layout_column="9"
                      android:gravity="center" android:layout_margin="1dp"
                      android:background="#ffffff"/>
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="16"
                      android:id="@+id/textView_zhoushu_16" android:layout_column="5"
                      android:gravity="center" android:layout_margin="1dp"
                      android:background="#ffffff"/>
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="17"
                      android:id="@+id/textView_zhoushu_17" android:layout_column="6"
                      android:gravity="center" android:layout_margin="1dp"
                      android:background="#ffffff"/>
            <TextView android:layout_width="40dp" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="18"
                      android:id="@+id/textView_zhoushu_18" android:layout_column="7"
                      android:gravity="center" android:layout_margin="1dp"
                      android:background="#ffffff"/>
